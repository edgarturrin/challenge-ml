  "scripts": {
    "start": "npm run build && npm run dev",
    "build": "tsc && mkdir -p dist/public && npm run build:css && npm run build:client",
    "dev": "node dist/src/index.js",
    "build:client": "npx esbuild src/web/client.tsx --bundle --outfile=dist/public/bundle.js --platform=browser --target=es2017",
    "build:css": "cat src/web/styles/*.css > dist/public/tmp.css && npx postcss dist/public/tmp.css -o dist/public/bundle.css && rm dist/public/tmp.css",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
